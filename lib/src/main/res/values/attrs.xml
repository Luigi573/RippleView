<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RippleView">
        <attr name="ripple_style" format="enum">
            <enum name="vertical" value="0x01"/>
            <enum name="radial" value="0x02"/>
        </attr>
        <attr name="ripple_color" format="color"/>
        <attr name="ripple_backgroundColor" format="color"/>
        <attr name="animate_duration" format="integer"/>
    </declare-styleable>
</resources>